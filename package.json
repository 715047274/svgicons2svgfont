{
  "name": "svgicons2svgfont",
  "version": "0.0.7",
  "description": "Read a set of SVG icons and ouput a SVG font",
  "homepage": "https://github.com/nfroidure/svgicons2svgfont",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha tests/*.mocha.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha --report html -- tests/*.mocha.js -R spec -t 5000"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/svgicons2svgfont"
  },
  "keywords": [
    "SVG",
    "icons",
    "font",
    "creation",
    "fonts"
  ],
	"dependencies": {
		"sax": "0.5.x",
		"svg-pathdata": "0.0.3"
	},
	"devDependencies": {
		"mocha": "1.x.x"
	},
  "author": {
    "name": "Nicolas Froidure",
    "url": "http://www.insertafter.com/blog.html"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/nfroidure/svgicons2svgfont/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/nfroidure/svgicons2svgfont/issues"
  },
  "preferGlobal": "true",
  "bin": {
    "svgicons2svgfont" : "bin/svgicons2svgfont.js"
  }
}
